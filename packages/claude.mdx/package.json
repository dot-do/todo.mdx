{
  "name": "claude.mdx",
  "version": "0.1.0",
  "description": "AI-assisted development orchestrator - dispatches Claude Code sessions to work on TODOs from beads issue tracker",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "claude.mdx": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "keywords": [
    "orchestrator",
    "claude",
    "ai",
    "development",
    "automation",
    "beads",
    "workflow",
    "cli"
  ],
  "author": "dot-do",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dot-do/todo.mdx.git",
    "directory": "packages/claude.mdx"
  },
  "dependencies": {
    "beads-workflows": "^0.1.1",
    "commander": "^12.1.0",
    "execa": "^9.5.2",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
