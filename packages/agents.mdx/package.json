{
  "name": "agents.mdx",
  "version": "0.1.0",
  "description": "Proxy runtime for workflow orchestration - abstracts local vs cloud execution",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./local": {
      "types": "./dist/local.d.ts",
      "import": "./dist/local.js"
    },
    "./cloud": {
      "types": "./dist/cloud.d.ts",
      "import": "./dist/cloud.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "keywords": [
    "agents",
    "mdx",
    "workflow",
    "orchestration",
    "claude",
    "proxy",
    "runtime"
  ],
  "author": "dot-do",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dot-do/todo.mdx.git",
    "directory": "packages/agents.mdx"
  },
  "dependencies": {
    "@octokit/rest": "^21.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/bun": "^1.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "beads-workflows": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "beads-workflows": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
