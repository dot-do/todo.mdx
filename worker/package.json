{
  "name": "todo-mdx-worker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --noEmit",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply todo-mdx",
    "db:studio": "drizzle-kit studio",
    "auth:generate": "npx @better-auth/cli generate --config src/auth.ts"
  },
  "dependencies": {
    "agents.mdx": "workspace:*",
    "better-auth": "^1.0.0",
    "drizzle-orm": "^0.41.0",
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.0.0",
    "@cloudflare/workers-types": "^4.20241205.0",
    "drizzle-kit": "^0.31.4",
    "typescript": "^5.7.0",
    "wrangler": "^3.99.0"
  }
}
